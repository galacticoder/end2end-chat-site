#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/someone/programming/end2end-chat-site/server/node_modules/semver/bin/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules/semver/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules:/home/someone/programming/end2end-chat-site/node_modules:/home/someone/programming/node_modules:/home/someone/node_modules:/home/node_modules:/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/someone/programming/end2end-chat-site/server/node_modules/semver/bin/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules/semver/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules:/home/someone/programming/end2end-chat-site/node_modules:/home/someone/programming/node_modules:/home/someone/node_modules:/home/node_modules:/node_modules:/home/someone/programming/end2end-chat-site/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
